<!--マンガ評価一覧　render部分-->
<table class="table table-hover">
  <thead>
    <tr>
      <th></th>
      <th>マンガのタイトル</th>
      <th>評価</th>
      <th>投稿者</th>
    </tr>
  </thead>
  <% mangas.each do |manga|%>
  <tbody>
    <tr>
      <td>
        <%= link_to manga_path(manga) do %>
          <%= attachment_image_tag manga, :image, :fill,60,60%>
        <% end %>
      </td>
      <td>
        <%= link_to manga_path(manga) do %>
          <%= manga.title%>
        <% end %>
      </td>
      <td><%#= manga.review%></td>
      <td><%= manga.member.nickname%></td>
    </tr>
  </tbody>
  <% end %>
</table>